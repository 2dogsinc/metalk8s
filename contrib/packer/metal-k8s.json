{
  "builders": [
    {
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 16384,
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "guest_os_type": "RedHat_64",
      "hard_drive_discard": true,
      "hard_drive_interface": "sata",
      "hard_drive_nonrotational": true,
      "http_directory": "http",
      "iso_checksum": "bba314624956961a2ea31dd460cd860a77911c1e0a56e4820a12b9c5dad363f5",
      "iso_checksum_type": "sha256",
      "iso_checksum_url": "http://mirrors.atosworldline.com/public/centos/7/isos/x86_64/sha256sum.txt",
      "iso_interface": "sata",
      "iso_urls": [
        "CentOS-7-x86_64-Minimal-1708.iso",
        "http://mirrors.atosworldline.com/public/centos/7/isos/x86_64/CentOS-7-x86_64-Minimal-1708.iso"
      ],
      "shutdown_command": "echo 'vagrant' | sudo -S /sbin/halt -h -p",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "120s",
      "type": "virtualbox-iso",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "8192"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ]
    }
  ],
  "post-processors": []
}

